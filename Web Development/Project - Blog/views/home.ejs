<%- include("partials/header") -%>
<form action="/compose">
    <button type="submit" class="btn btn-default btn-circle btn-xl">+</button>
</form>
<h1>Home</h1>
<p><%= startingHomeContent %></p>

<% posts.forEach((post) => { %>
    <h1><%= post.title %> </h1>
    <p>
        <%= post.body.slice(0, 100) + " ..." %>
        <a href="/posts/<%= post.title %>">Read More</a>
    </p>
<% }); %>
<%- include("partials/footer") -%>
 